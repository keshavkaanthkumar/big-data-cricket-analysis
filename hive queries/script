CREATE  TABLE cricketscores (Innings_Player String,Innings_Runs_Scored String,Innings_Runs_Scored_Num Int,Innings_Minutes_Batted String,Innings_Batted_Flag String,Innings_Not_Out_Flag String,Innings_Balls_Faced String,Innings_Boundary_Fours String,Innings_Boundary_Sixes String,Innings_Batting_Strike_Rate String,Innings_Number String,Opposition String,Ground String,Innings_Date String,Country String,fifty String,hundreds String,Innings_Runs_Scored_Buckets String,Innings_Overs_Bowled String,Innings_Bowled_Flag String,Innings_Maidens_Bowled String,Innings_Runs_Conceded String,Innings_Wickets_Taken String,four_Wickets String,five_Wickets String,ten_Wickets String,Innings_Wickets_Taken_Buckets String,Innings_Economy_Rate String) ROW FORMAT DELIMITED FIELDS TERMINATED BY ',' LOCATION '/dataset';

Select Innings_Player, Ground FROM cricketscores GROUP BY Innings_Player;
Select Innings_Player, Ground,count(*) as times FROM cricketscores where Innings_Runs_Scored_Num > 100 GROUP BY Innings_Player, Ground ORDER BY times DESC LIMIT 20;
